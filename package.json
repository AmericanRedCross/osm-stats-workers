{
  "name": "osm-stats-workers",
  "version": "0.2.0",
  "description": "Summarize OSM data to metrics",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test tap test/*.test.js test/*/*.test.js --cov",
    "lint": "eslint index.js src/*",
    "azure-pack": "npm run clean && NODE_ENV=production webpack --progress --display-optimization-bailout --config webpack.config.js",
    "clean": "rm -f dist/azure/*/index.js*",
    "azure-publish": "npm run azure-pack && gh-pages -d dist/azure -b azure-next",
    "housekeeping": "node bin/housekeeping.js",
    "osm-changes": "node bin/osm-changes.js",
    "update-badges": "node bin/update-badges.js",
    "start": "npm run osm-changes"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6.10.0"
  },
  "dependencies": {
    "@turf/buffer": "^5.1.5",
    "@turf/helpers": "^6.0.1",
    "@turf/length": "^5.1.5",
    "@turf/meta": "^6.0.1",
    "async": "^2.6.0",
    "babel-polyfill": "^6.26.0",
    "epipebomb": "^1.0.0",
    "geojson-rbush": "^3.1.0",
    "highland": "^2.11.1",
    "martinez-polygon-clipping": "^0.3.3",
    "osm-replication-streams": "^0.6.2",
    "osm2obj": "^3.0.0",
    "pg": "^7.4.1",
    "pg-query-stream": "^1.1.1",
    "pg.js": "^4.1.1",
    "require-env": "^0.2.1"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "gh-pages": "^1.1.0",
    "nodemon": "^1.14.12",
    "pgexplode": "^1.0.1",
    "prettier": "^1.10.2",
    "tap": "^11.1.0",
    "webpack": "^3.11.0"
  }
}
