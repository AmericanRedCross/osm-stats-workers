{
  "name": "osm-stats-workers",
  "version": "0.2.0",
  "description": "Summarize OSM data to metrics",
  "main": "app.js",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test tap test/*.test.js test/*/*.test.js --cov",
    "lint": "eslint index.js src/*",
    "dbinit": "./bin/database_init.sh",
    "azure-pack": "NODE_ENV=production webpack --progress --display-optimization-bailout --config webpack.config.js",
    "clean": "rm -f dist/azure/worker/index.js*",
    "azure-publish": "npm run azure-pack && gh-pages -d dist/azure -b azure"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.0.6",
    "bookshelf": "^0.11.1",
    "bookshelf-json-columns": "^2.1.1",
    "country-reverse-geocoding": "^0.2.2",
    "dotenv": "^2.0.0",
    "fs": "0.0.2",
    "geojson-precision": "^0.4.0",
    "install": "^0.4.1",
    "knex": "^0.14.1",
    "moment": "^2.10.6",
    "npm": "^3.5.2",
    "osm-replication-streams": "^0.2.2",
    "pg": "^7.4.0",
    "ramda": "^0.19.0",
    "request-promise": "^2.0.1",
    "require-env": "^0.2.1",
    "turf": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "gh-pages": "^1.1.0",
    "tap": "^2.3.1",
    "webpack": "^3.8.1"
  }
}
