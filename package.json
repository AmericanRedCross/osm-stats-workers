{
  "name": "osm-stats-workers",
  "version": "0.2.0",
  "description": "Summarize OSM data to metrics",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test tap test/*.test.js test/*/*.test.js --cov",
    "lint": "eslint index.js src/*",
    "dbinit": "./bin/database_init.sh",
    "azure-pack": "NODE_ENV=production webpack --progress --display-optimization-bailout --config webpack.config.js",
    "clean": "rm -f dist/azure/worker/index.js*",
    "azure-publish": "npm run azure-pack && gh-pages -d dist/azure -b azure"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6.10.0"
  },
  "dependencies": {
    "@turf/buffer": "^5.1.5",
    "@turf/helpers": "^6.0.0-beta.3",
    "@turf/length": "^5.1.5",
    "async": "^2.6.0",
    "babel-polyfill": "^6.26.0",
    "epipebomb": "^1.0.0",
    "geojson-rbush": "^2.2.1",
    "highland": "^2.11.1",
    "martinez-polygon-clipping": "^0.3.2",
    "osm-replication-streams": "^0.5.1",
    "osm2obj": "^3.0.0",
    "pg": "^7.4.1",
    "ramda": "^0.19.0",
    "require-env": "^0.2.1"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.10.2",
    "tap": "^2.3.1",
    "webpack": "^3.8.1"
  }
}
